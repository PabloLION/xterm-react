{
  "name": "@pablo-lion/xterm-react",
  "version": "1.2.0",
  "description": "A React component for xterm.js terminal integration.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "lint": "eslint --fix .",
    "lint:no-fix": "eslint .",
    "format": "prettier --write .",
    "biome:check": "biome check .",
    "biome:fix": "biome check --fix .",
    "biome:format": "biome format --write .",
    "build": "npx rimraf dist && tsc",
    "test": "echo \"Error: no test specified, maybe you want to use `dev`\" && exit 1",
    "test:versions": "node version-compatibility-tests/run-all-tests.js",
    "test:react": "node version-compatibility-tests/core/react-versions-test.js",
    "test:eslint": "node version-compatibility-tests/devtools/eslint-versions-test.js",
    "test:biome": "node version-compatibility-tests/devtools/biome-versions-test.js",
    "publish": "./scripts/prepare-publish.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@eslint/js": "^9.4.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "prettier": "^3.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0",
    "vite": "^7.1.2"
  },
  "dependencies": {
    "@xterm/xterm": "^5.5.0"
  },
  "keywords": [
    "xterm",
    "terminal",
    "console",
    "react",
    "xterm.js",
    "component"
  ],
  "author": "PabloLION <https://github.com/PabloLION/>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PabloLION/xterm-react.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false
}
